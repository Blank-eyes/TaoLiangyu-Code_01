<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.study.mapper.StudentMapper.java">
 
   <select id="getStudentById" parameterType="Integer" resultType="StudentBean">
     	SELECT
		  student_id,
		  student_name,
		  student_sex,
		  student_age
		FROM studentinfo
		WHERE student_id =#{student_id}
   </select>
 </mapper>